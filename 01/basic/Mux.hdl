/** 
 * Multiplexor:
 * If sel=0 then out=a else out=b.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=a, b=notsel, out=o0);
    And(a=b, b=sel, out=o1);
    Or(a=o0, b=o1, out=out);
}
